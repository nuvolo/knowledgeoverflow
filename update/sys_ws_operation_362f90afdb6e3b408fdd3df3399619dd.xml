        <http_method>GET</http_method>
        <name>Get Profile</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
	var type = request.queryParams.type[0];
	var filter = request.queryParams.filter[0];
	var page = request.queryParams.page[0];
	var questionUtil = new x_nuvo_ko.QuestionUtil();
	var tagUtil = new x_nuvo_ko.TagUtil();
	switch(type){
		case 'starred':
			return questionUtil.getStarred(filter, page);
		case 'tags':
			return tagUtil.getWatchedQuestions(filter, page);
		case 'answers':
			return questionUtil.getProfileAnswers(filter, page);
		case 'questions':
			return questionUtil.getProfileQuestions(filter, page);
		default:
			return questionUtil.getProfileQuestions(filter, page);
	}

})(request, response);]]></operation_script>
