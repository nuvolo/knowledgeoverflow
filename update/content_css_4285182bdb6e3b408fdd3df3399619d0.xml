}&#13;
&#13;
/*MAIN PAGE CSS*/&#13;
@media (max-width:750px) {&#13;
    .left-sidebar {&#13;
        display: none;&#13;
    }&#13;
&#13;
    .right-sidebar {&#13;
        display: none;&#13;
    }&#13;
}&#13;
&#13;
@media (max-width:950px) {&#13;
